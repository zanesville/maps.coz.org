    "clean-docs": "gulp clean-jsdoc",
    "test-deploy": "netlify deploy --prod",
    "encrypt": "node _scripts/_encrypt/encrypt.js",
    "deploy:test": "hexo clean && npm run parcel-css && npm run build && npm run clean-docs && npm run docs && hexo generate && node _scripts/genPublicApps && gulp clean-public && node _scripts/localToCDN && netlify deploy --dir public",
